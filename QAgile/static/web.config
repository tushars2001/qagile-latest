<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.webServer>
		<handlers>
			<clear/>
			<add name="StaticFile" path="*" verb="*" module="StaticFileModule" resourceType="File" requireAccess="Read" />
		</handlers>
		<httpProtocol>
    <customHeaders>
      <add name="X-Frame-Options" value="SAMEORIGIN" />
    </customHeaders>
  </httpProtocol>
	</system.webServer>
</configuration>